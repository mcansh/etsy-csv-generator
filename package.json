    "dotenv": "8.2.0",
    "he": "1.2.0",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@types/he": "1.1.1",
    "@types/node": "14.11.2",
    "@types/node-fetch": "2.5.7",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "typescript": "4.0.3"
  },
  "husky": {
